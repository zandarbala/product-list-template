{
  "mobileJson": {
    "canvasWidth": 310,
    "imageHeight": "116px",
    "renderImage": "./assets/tools/home_sections.png",
    "verticalPosition": "450px",
    "horizontalPosition": "240px"
},
  "desktopJson": {
    "domTree": {
      "tag": "div",
      "style": {
        "gap": "10px",
        "width": "100%",
        "height": "150vh",
        "display": "flex",
        "position": "relative",
        "justifyContent": "space-between"
      },
      "scripts": {
        "handleClick": {
          "body": "const event = new CustomEvent('productListHandleClick', {detail:value});document.dispatchEvent(event);",
          "params": [
            "value"
          ]
        }
      },
      "cssStyles": {
        ".zwe-pl-add-to-cart-btn:hover,.add-to-cart-icon:hover ~ .zwe-pl-add-to-cart-btn": {
          "backgroundColor": "white !important",
          "color": "black !important",
          "border": "1px dotted var(--element-primary-color,grey) !important"
        },
        ".zwe-pl-add-to-cart-btn:hover ~ .add-to-cart-icon": {
          "color": "black !important"
        },
        ".productTitle:hover": {
          "textDecoration": "underline !important"
        },
        ".productImage": {
          "transform": "scale(1)",
          "transition": "transform 0.3s ease-in-out"
        },
        ".productImage:hover": {
          "transform": "scale(1.1)"
        },
        ".productquickView:hover ~ .productImage": {
          "transform": "scale(1.1)"
        },
        ".product-list-img-container:hover ~ .productquickView, .productquickView:hover": {
          "opacity": "1 !important",
          "transform": "translateY(0)",
          "zIndex": "1 !important",
          "cursor": "pointer"
        },
        ".productquickView": {
          "opacity": "0",
          "transform": "translateY(100%)",
          "transition": "transform 0.3s ease-in-out, opacity 0.3s ease-in-out",
          "position": "absolute",
          "zIndex": "0",
          "pointerEvents": "none"
        }
      },
      "children": [
        {
          "tag": "zwe-product-filter",
          "style": {
            "top": "0",
            "left": "0",
            "width": "25%",
            "height": "100%"
          },
          "attributes": {
            "id": "mbrtfype_1nqkn",
            "data-role": "webComponent"
          },
          "configuration": {
            "filterData": {
              "sortArray": [
                {
                  "value": {
                    "order": "ASC",
                    "fieldName": "name"
                  },
                  "display": "Name : Ascending",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "DESC",
                    "fieldName": "name"
                  },
                  "display": "Name : Descending",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "DESC",
                    "fieldName": "price"
                  },
                  "display": "Price : Highest first",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "ASC",
                    "fieldName": "price"
                  },
                  "display": "Price : Lowest first",
                  "isVisible": true
                }
              ],
              "filterArray": [
                {
                  "type": "check-box",
                  "options": [
                    {
                      "value": {
                        "end": 499,
                        "begin": 0
                      },
                      "display": "$0 - $499"
                    },
                    {
                      "value": {
                        "end": 999,
                        "begin": 500
                      },
                      "display": "$500 - $999"
                    },
                    {
                      "value": {
                        "end": 1999,
                        "begin": 1000
                      },
                      "display": "$1000 - $1999"
                    },
                    {
                      "value": {
                        "end": 3999,
                        "begin": 2000
                      },
                      "display": "$2000 - $3999"
                    },
                    {
                      "value": {
                        "end": 10000,
                        "begin": 4000
                      },
                      "display": "$4000 - $10000"
                    }
                  ],
                  "fieldName": "price",
                  "isDefault": true,
                  "isVisible": true,
                  "filterName": "Price",
                  "optionType": "Range"
                },
                {
                  "type": "radio-option",
                  "options": [
                    {
                      "value": {
                        "op": ">",
                        "value": 4
                      },
                      "display": "4 stars & above"
                    },
                    {
                      "value": {
                        "op": ">",
                        "value": 3
                      },
                      "display": "3 stars & above"
                    },
                    {
                      "value": {
                        "op": ">",
                        "value": 2
                      },
                      "display": "2 stars & above"
                    },
                    {
                      "value": {
                        "op": ">",
                        "value": 1
                      },
                      "display": "1 star & above"
                    }
                  ],
                  "fieldName": "rating",
                  "isDefault": true,
                  "isVisible": true,
                  "filterName": "Customer Ratings",
                  "optionType": "Condition"
                },
                {
                  "type": "radio-option",
                  "options": [
                    {
                      "value": "inStock",
                      "display": "Instock"
                    },
                    {
                      "value": "outOfStock",
                      "display": "Out of stock"
                    }
                  ],
                  "fieldName": "stock",
                  "isDefault": true,
                  "isVisible": true,
                  "filterName": "Stock",
                  "optionType": "Simple"
                }
              ]
            },
            "filterMode": "normal",
            "filterTemplate": "filterTemplateOne",
            "showFoodFilter": false,
            "showSortOption": false,
            "showFilterButton": false,
            "showFilterSection": true,
            "filterTemplateTextColor": "",
            "filterTemplateBackgroundColor": ""
          }
        },
        {
          "tag": "zb-product-list",
          "card": {
            "tag": "div",
            "style": {
              "width": "290px",
              "height": "424px",
              "position": "relative",
              "boxShadow": "0px 0px 2px 2px rgba(0, 0, 0, 0.1), 0px 0px 0px 0px rgba(0, 0, 0, 0), 0px 0px 0px 0px rgba(0, 0, 0, 0)"
            },
            "children": [
              {
                "tag": "div",
                "style": {
                  "maxWidth": "290px",
                  "height": "290px",
                  "position": "absolute",
                  "overflow": "hidden",
                  "cursor": "pointer"
                },
                "children": [
                  {
                    "tag": "img",
                    "style": {
                      "width": "100%",
                      "height": "100%",
                      "position": "relative",
                      "objectFit": "contain"
                    },
                    "attributes": {
                      "id": "mbrtfype_6dkbn",
                      "alt": "${name}",
                      "src": "${ featuredImage ? awsUrl+featuredImage : 'assets/image-not-available.png'}",
                      "type": "image",
                      "class": "productImage",
                      "isEnabled": true,
                      "data-role": "productImage",
                      "zb-other-angle": true
                    }
                  }
                ],
                "attributes": {
                  "isEnabled": true,
                  "data-role": "productImage",
                  "type": "image",
                  "class": "product-list-img-container"
                }
              },
              {
                "tag": "a",
                "style": {
                  "position": "absolute",
                  "padding": "5px 12px",
                  "height": "auto",
                  "top": "295px",
                  "width": "190px",
                  "whiteSpace": "nowrap",
                  "color": "var(--dark-color,black)",
                  "lineHeight": "1.5",
                  "overflow": "hidden",
                  "cursor": "pointer",
                  "textOverflow": "ellipsis",
                  "display": "-webkit-box",
                  "wordBreak": "break-word",
                  "WebkitLineClamp": "2",
                  "WebkitBoxOrient": "vertical",
                  "fontStyle": "italic",
                  "fontWeight": "400",
                  "fontSize": "18px",
                  "fontFamily": "'Playfair Display', serif"
                },
                "attributes": {
                  "id": "mbrtfype_b2lb0",
                  "type": "text",
                  "class": "productTitle ",
                  "isEnabled": true,
                  "title": "${name}",
                  "data-role": "productName"
                },
                "textContent": "${name}"
              },
              {
                "tag": "span",
                "condition": "${weightUnit}",
                "style": {
                  "position": "absolute",
                  "top": "328px",
                  "left": "72px",
                  "width": "90px",
                  "height": "25px",
                  "padding": "5px 12px",
                  "fontStyle": "italic",
                  "fontWeight": "700",
                  "fontSize": "18px",
                  "color": "rgb(141, 17, 39)",
                  "fontFamily": "'Playfair Display', serif"
                },
                "attributes": {
                  "id": "mbrtfype_pxcpv",
                  "type": "text",
                  "class": "productPrice ",
                  "isEnabled": true,
                  "data-role": "productWeightUnit"
                },
                "textContent": "/ ${weightUnit}"
              },
              {
                "tag": "span",
                "style": {
                  "position": "absolute",
                  "top": "328px",
                  "width": "90px",
                  "height": "25px",
                  "padding": "5px 12px",
                  "color": "#8d1127",
                  "fontStyle": "italic",
                  "fontWeight": "700",
                  "fontSize": "18px",
                  "fontFamily": "'Playfair Display', serif"
                },
                "attributes": {
                  "id": "mbrtfype_pxcpv",
                  "type": "text",
                  "class": "productPrice ",
                  "isEnabled": true,
                  "data-role": "productPrice"
                },
                "textContent": "${currency}${price}"
              },
              {
                "tag": "span",
                "style": {
                  "top": "351px",
                  "width": "100px",
                  "height": "25px",
                  "position": "absolute",
                  "textDecoration": "line-through",
                  "alignItems": "center",
                  "marginTop": "2px",
                  "font-size": "calc(var(--product-list-font-size,15px) - 2px)",
                  "display": "${costComparePrice ? 'inline':'none'}",
                  "fontStyle": "italic",
                  "fontWeight": "700",
                  "fontSize": "18px",
                  "fontFamily": "'Playfair Display', serif",
                  "padding": "5px 12px",
                  "color": "#8d1127"
                },
                "attributes": {
                  "id": "mbrtfype_lcuai",
                  "type": "text",
                  "class": "productCostComparePrice ",
                  "isEnabled": true,
                  "data-role": "productCostComparePrice"
                },
                "textContent": "${currency}${costComparePrice}",
                "condition": "${costComparePrice}"
              },
              {
                "tag": "span",
                "style": {
                  "top": "304px",
                  "left": "215px",
                  "width": "80px",
                  "height": "25px",
                  "position": "absolute",
                  "fontFamily": "Arial",
                  "fontSize": "var(--product-list-font-size,15px)",
                  "fontWeight": "700",
                  "color": "var(--element-primary-color,grey)"
                },
                "attributes": {
                  "id": "mbrtfype_77pkg",
                  "type": "text",
                  "class": "discountPercentage",
                  "isEnabled": true,
                  "data-role": "productDiscount"
                },
                "textContent": " % OFF"
              },
              {
                "tag": "span",
                "style": {
                  "top": "308px",
                  "left": "7px",
                  "width": "280px",
                  "height": "45px",
                  "position": "absolute",
                  "fontSize": "16px",
                  "color": "#000000",
                  "fontFamily": "Arial",
                  "fontWeight": "400",
                  "display": "none"
                },
                "attributes": {
                  "id": "mbrtfype_9jiii",
                  "type": "text",
                  "class": "productDescription ",
                  "isEnabled": false,
                  "data-role": "productDescription"
                },
                "condition": "${shortDescription}",
                "textContent": "${shortDescription}"
              },
              {
                "tag": "button",
                "attributes": {
                  "type": "text",
                  "data-role": "productQuickView",
                  "isEnabled": true
                },
                "condition": "${!isDigitalProduct}",
                "style": {
                  "backgroundColor": "var(--element-primary-color,grey)",
                  "borderRadius": "10px",
                  "border": "none",
                  "padding": "5px 12px",
                  "color": "white",
                  "top": "10px",
                  "left": "10px",
                  "position": "absolute"
                },
                "textContent": "Quick View",
                "events": {
                  "click": {
                    "function": "handleClick",
                    "params": [
                      "quickView"
                    ]
                  }
                }
              },
              {
                "tag": "button",
                "condition": "${multiLocationVariantStocks[0]?.stock <= 0}",
                "style": {
                  "top": "388px",
                  "align-item": "center",
                  "width": "150px",
                  "left": "70px",
                  "height": "30px",
                  "position": "absolute",
                  "fontSize": "14px",
                  "color": "white",
                  "fontWeight": "700",
                  "fontFamily": "'Montserrat', sans-serif",
                  "border": "1px solid var(--element-primary-color,black)",
                  "borderRadius": "10px",
                  "backgroundColor": "var(--element-primary-color,grey)",
                  "transition": "transform 0.3s ease-in-out, opacity 0.3s ease-in-out"
                },
                "attributes": {
                  "class": "zwe-pl-out-of-stock-btn"
                },
                "textContent": "Out of stock"
              },
              {
                "tag": "button",
                "condition": "${((multiLocationVariantStocks?.[0]?.stock == -1) || (multiLocationVariantStocks?.[0]?.stock >=1)) && !isDigitalProduct}",
                "style": {
                  "top": "388px",
                  "alignItem": "center",
                  "width": "260px",
                  "left": "15px",
                  "height": "27px",
                  "position": "absolute",
                  "display": "flex",
                  "flexDirection": "row",
                  "fontSize": "11px",
                  "color": "white",
                  "fontWeight": "700",
                  "cursor": "pointer",
                  "fontFamily": "Montserrat, sans-serif",
                  "border": "1px solid var(--element-primary-color,black)",
                  "borderRadius": "2px",
                  "alignItems": "center",
                  "justify-content": "center",
                  "gap": "10px",
                  "backgroundColor": "var(--element-primary-color,grey)",
                  "transition": "transform 0.3s ease-in-out, opacity 0.3s ease-in-out"
                },
                "attributes": {
                  "class": "zwe-pl-add-to-cart-btn",
                  "data-role": "productCartButton",
                  "isEnabled": true
                },
                "children": [
                  {
                    "tag": "div",
                    "style": {
                      "order": "1"
                    },
                    "attribute": {
                      "type": "text"
                    },
                    "textContent": "ADD TO CART"
                  },
                  {
                    "tag": "span",
                    "attributes": {
                      "data-id": "opaline-tiles-video-control-section",
                      "class": "material-symbols-outlined fill add-to-cart-icon",
                      "type": "icon"
                    },
                    "condition": "${multiLocationVariantStocks[0]?.stock >= 1}",
                    "style": {
                      "order": "2",
                      "zIndex": "1",
                      "fontSize": "17px",
                      "borderRadius": "50%",
                      "cursor": "pointer"
                    },
                    "attribute": {
                      "type": "icon"
                    },
                    "textContent": "shopping_cart"
                  }
                ]
              }
            ],
            "attributes": {
              "id": "mbrtfype_jyd2w",
              "class": "zwe-product-card container",
              "productIndex": "${index}"
            },
            "events": {
              "click": {
                "function": "handleClick",
                "params": [
                  "navigateTo"
                ]
              }
            }
          },
          "style": {
            "top": "0",
            "right": "0",
            "width": "75%",
            "height": "100%",
            "overflow": "hidden scroll"
          },
          "pageName": "Product list",
          "attributes": {
            "id": "mbrtfype_s4yhm",
            "data-role": "webComponent",
            "discountFormat": "{discountPercentage}% OFF"
          },
          "elementsList": [
            {
              "id": "product-list-container",
              "tag": "box",
              "elementName": "Box",
              "isWebElement": true,
              "updatedJsonName": "boxStyles"
            }
          ],
          "configuration": {
            "api": "featuredProducts",
            "datas": [
              "name",
              "price",
              "featuredImage",
              "costComparePrice"
            ],
            "productCardDesign": "tilesFriendlyDesign",
            "columnGap": "",
            "backgroundColor": ""
          },
          "isConfigDialog": true
        }
      ],
      "attributes": {
        "id": "mbrtfype_2cidh"
      }
    },
    "canvasWidth": 1351,
    "imageHeight": "116px",
    "renderImage": "./assets/tools/home_sections.png",
    "verticalPosition": "450px",
    "horizontalPosition": "240px"
  },
  "sectionCode": "PRODUCT_LIST",
  "sectionName": "Product List"
}